{
  "name": "telephone-ai-anywhere",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "prebuild": "expo prebuild",
    "build:apk:local": "expo prebuild --platform android && cd android && gradlew assembleRelease",
    "build:apk:debug": "expo prebuild --platform android && cd android && gradlew assembleDebug",
    "build:apk:eas": "eas build -p android --profile preview",
    "build:aab": "eas build -p android --profile production",
    "clean": "expo prebuild --clean",
    "lint": "eslint .",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.5",
    "expo-av": "~15.0.0",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.0",
    "expo-crypto": "~14.0.0",
    "expo-document-picker": "~13.0.0",
    "expo-file-system": "~18.0.0",
    "expo-haptics": "~14.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-sharing": "~13.0.0",
    "expo-speech": "~13.0.0",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "~5.3.0"
  },
  "private": true
}
